# RouterOS API Configuration
ROUTEROS_HOST=<ip_or_hostname_url>
ROUTEROS_USER=<api-monitor-user>
ROUTEROS_API_PROTOCOL=<http or https>
ROUTEROS_PASS_PATH=<pass/path>

# Zaram ONT Module Configuration
ZARAM_ONT_IP=192.168.200.1
ZARAM_ONT_USER=admin
ZARAM_PASS_PATH=<pass/path>

# Monitoring Configuration
MONITORED_INTERFACES=<your_interfaces_ie_sfp-sfpplus1,pppoe-wan>
COLLECTION_INTERVAL_SECONDS=30
METRICS_PORT=9700
METRICS_HOST=0.0.0.0

# SSH Configuration
SSH_USER=<your_user>
SSH_HOST=<ip_or_hostname_url>

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/sfp_monitor.log
LOG_MAX_BYTES=1048576
LOG_BACKUP_COUNT=5
DEBUG_LOGGING=false